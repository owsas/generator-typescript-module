{
  "name": "generator-typescript-module",
  "version": "0.0.3",
  "description": "Generator for typescript modules",
  "homepage": "",
  "author": {
    "name": "Juan Camilo Guarín Peñaranda",
    "email": "jcguarinpenaranda@gmail.com",
    "url": "http://owsas.com"
  },
  "files": [
    "generators"
  ],
  "main": "generators/index.js",
  "keywords": [
    "npm",
    "typescript",
    "module",
    "generator",
    "yeoman",
    "test",
    "yeoman-generator"
  ],
  "devDependencies": {
    "yeoman-test": "^1.7.0",
    "yeoman-assert": "^3.1.0",
    "eslint": "^5.4.0",
    "prettier": "^1.11.1",
    "husky": "^0.14.3",
    "lint-staged": "^7.2.2",
    "eslint-config-prettier": "^3.0.1",
    "eslint-plugin-prettier": "^2.6.0",
    "eslint-config-xo": "^0.24.2",
    "jest": "^23.5.0"
  },
  "engines": {
    "npm": ">= 4.0.0"
  },
  "dependencies": {
    "yeoman-generator": "^2.0.1",
    "chalk": "^2.1.0",
    "yosay": "^2.0.1"
  },
  "jest": {
    "testEnvironment": "node"
  },
  "lint-staged": {
    "*.js": [
      "eslint --fix",
      "git add"
    ],
    "*.json": [
      "prettier --write",
      "git add"
    ]
  },
  "eslintConfig": {
    "extends": [
      "xo",
      "prettier"
    ],
    "env": {
      "jest": true,
      "node": true
    },
    "rules": {
      "prettier/prettier": [
        "error",
        {
          "singleQuote": true,
          "printWidth": 90
        }
      ],
      "capitalized-comments": 0
    },
    "plugins": [
      "prettier"
    ]
  },
  "scripts": {
    "pretest": "eslint .",
    "precommit": "lint-staged",
    "test": "jest"
  },
  "repository": "owsas/generator-typescript-module",
  "license": "MIT"
}
